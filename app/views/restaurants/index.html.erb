<h1>Restaurants#index</h1>
<p>Find me in app/views/restaurants/index.html.erb</p>
<h3><%= link_to "Add New Restaurant", new_restaurant_path  %></h3>

<% @restaurants.each do |restaurant| %>
  <ul>
    <li>Name: <%= restaurant.name %></li>
    <li>Address: <%= restaurant.address %></li>
    <li>Phone Number: <%= restaurant.phone_number %></li>
    <li>Category: <%= restaurant.category %></li>

    <hr>
  </ul>
<% end %>
